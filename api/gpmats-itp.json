{
    "openapi": "3.0.1",
    "info": {
        "title": "eRA PAS Web Service API",
        "description": "eRA PAS Web Service",
        "license": {
            "name": "Apache 2.0",
            "url": "http://springdoc.org"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://web-dev.aic.era.nih.gov",
            "description": "development"
        },
        {
            "url": "https://web-test.aic.era.nih.gov",
            "description": "test"
        },
        {
            "url": "https://web-stage.aic.era.nih.gov",
            "description": "staging"
        },
        {
            "url": "https://web.aic.era.nih.gov",
            "description": "production"
        }
    ],
    "paths": {
        "/payplan/dataservices/itp-external/itp-update": {
            "post": {
                "tags": [
                    "gpmats-itp-controller"
                ],
                "operationId": "itpUpdate",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ItpInformation"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ItpUpdateResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Unauthorized",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server Error",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ItpInformation": {
                "type": "object",
                "properties": {
                    "applId": {
                        "type": "string"
                    },
                    "phsOrgCode": {
                        "type": "string"
                    },
                    "itpFlag": {
                        "type": "string",
                        "enum": [
                            "Y",
                            "N",
                            "D",
                            "NULL"
                        ]
                    },
                    "councilRecomCode": {
                        "type": "string",
                        "enum": [
                            "1",
                            "2",
                            "3",
                            "4",
                            "5",
                            "6",
                            "7",
                            "8",
                            "9",
                            "NULL"
                        ]
                    },
                    "fiscalYear": {
                        "type": "string"
                    },
                    "councilPriority": {
                        "type": "string",
                        "enum": [
                            "CON",
                            "HPP",
                            "LPP",
                            "NRC",
                            "OTH",
                            "NULL"
                        ]
                    }
                }
            },
            "ItpUpdateResponse": {
                "type": "object",
                "properties": {
                    "responseCode": {
                        "type": "boolean"
                    },
                    "errorMessages": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ItpMessage"
                        }
                    }
                }
            },
            "ItpMessage": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        }
    }
}